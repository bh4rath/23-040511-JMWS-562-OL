              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">String getHomePageCallResponse = new String(data);
int iterationCounter = Integer.parseInt(vars.get(&quot;counter&quot;));

log.info(&quot;Starting Iteration is &quot; + iterationCounter);
log.info(&quot;Checking the presence of \&quot;Example Domain\&quot; in the Response Body&quot; );

boolean stringFound = getHomePageCallResponse.contains(&quot;Example Domain&quot;);
log.info (&quot;String found&quot; + stringFound);

if(!stringFound)
{
prev.setSuccessful(false);
String errorMsg = &quot;Desired string \&quot;Example Domain\&quot; is not found&quot;;
prev.setResponseMessage(errorMsg);
log.error(errorMsg);
prev.setStopTestNow(true);
}

if(iterationCounter==3)
{
log.info(&quot;Completed first iteration successfully&quot;);
stopTest();
}


void stopTest()
{prev.setStopTest(true);}
</stringProp>
